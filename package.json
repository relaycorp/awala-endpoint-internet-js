{
  "name": "@relaycorp/awala-endpoint-internet",
  "version": "1.0.0",
  "author": {
    "email": "no-reply@relaycorp.tech",
    "name": "Relaycorp, Inc.",
    "url": "https://relaycorp.tech/"
  },
  "description": "JS library to make it easier for a server-side app to exchange messages with the Awala Internet Endpoint",
  "type": "module",
  "main": "build/lib/index.js",
  "typings": "build/lib/index.d.ts",
  "repository": "https://github.com/relaycorp/awala-endpoint-internet-js",
  "homepage": "https://docs.relaycorp.tech/awala-endpoint-internet-js/",
  "license": "MIT",
  "keywords": [
    "awala",
    "cloudevents",
    "pubsub",
    "gcp-pubsub",
    "google-pubsub"
  ],
  "scripts": {
    "build": "run-s clean && run-p build:main",
    "build:main": "tsc -p tsconfig.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier . --write",
    "fix:eslint": "eslint --fix src",
    "static-checks": "run-p static-checks:*",
    "static-checks:lint": "eslint src",
    "static-checks:prettier": "prettier . --list-different",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:integration": "exit 0",
    "doc-api": "typedoc src/index.ts --out build/docs",
    "clean": "del-cli build test"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.0",
    "@relaycorp/eslint-config": "^1.1.120",
    "@relaycorp/shared-config": "^1.9.4",
    "@types/jest": "^29.5.2",
    "del-cli": "^5.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.5.0",
    "jest-extended": "^4.0.0",
    "npm-run-all2": "^6.0.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "@relaycorp"
  },
  "prettier": "@relaycorp/shared-config/.prettierrc.json"
}
